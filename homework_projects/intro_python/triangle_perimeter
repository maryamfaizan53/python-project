def main():
    
    input1 : int = float(input("Enter ythe lenght if side 1: "))
    input2 : int = float(input("Enter ythe lenght if side 2: "))
    input3 : int = float(input("Enter ythe lenght if side 3: "))
    
    perimeter : int = input1 + input2 + input3
    print(f'The perimeter of the triangle is {perimeter}')
    
if __name__ == "__main__":
    main()